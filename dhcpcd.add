
# IP Configuration for Ethernet==========================================
interface eth0
static ip_address=192.168.0.160
static routers=192.168.0.1
static domain_name_servers=192.168.0.1 8.8.8.8
static netmask=255.255.255.0

# IP Configuration for wifi==============================================
interface wlan0
